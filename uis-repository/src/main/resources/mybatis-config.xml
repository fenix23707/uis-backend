<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="logImpl" value="STDOUT_LOGGING"/>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>

    <typeAliases>
        <typeAlias alias="User" type="by.kovzov.uis.domain.model.user.User"/>
        <typeAlias alias="UserRole" type="by.kovzov.uis.domain.model.user.UserRole"/>
    </typeAliases>

    <typeHandlers>
        <package name="by.kovzov.uis.repository.mybatis.handler"/>
    </typeHandlers>

    <mappers>
        <mapper resource="mapper/UserMapper.xml"/>
    </mappers>
</configuration>